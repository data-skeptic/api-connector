[API]
user: [Insert your username]

password: [Insert your password]

baseurl: http://home-sales-data-api.herokuapp.com

# This is the amount of time to sleep between calls to the API
waittime: 0.05

# Should your client do geocoding (preferred)?
# Values:
#   failsafe - Try to geocode, but on failure, send without this information (default)
#   failsecure - Try to geocode, quit on failure
#   none - Do not try to geocode
geocode: failsafe
